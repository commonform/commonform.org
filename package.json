  "private": true,
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:css": "node-sass --output-style compressed styles/styles.sass > build/styles.css",
    "build:licenses": "brlicenses -f full index.js > build/LICENSES",
    "build:js": "NODE_ENV=production browserify -p [ browserify-header --file header.js ] -p licensify -g unassertify -g es2020 -g uglifyify index.js | uglifyjs --comments warnings=false > build/browser.js",
    "chromedriver": "chromedriver --url-base=/wd/hub",
    "start": "node server.js",
    "test": "tape test/*.test.js",
    "lint": "standard --env browser",
    "licensee": "licensee 2>&1",
    "watch:css": "node-sass --watch --source-map true --output build styles/styles.sass",
    "watch:js": "watchify index.js -o build/browser.js -dv"
  }
}
